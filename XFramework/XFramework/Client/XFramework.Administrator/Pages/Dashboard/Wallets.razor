@page "/Wallets"
@inherits WalletsBase

<h1 class="text-center">Wallets List</h1>

<table class="table">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Birthday</th>
      <th>Address</th>
    </tr>
  </thead>
  <tbody>
   @foreach (var item in Model)
   {
     <tr>
       <td>@item.FirstName</td>
       <td>@item.LastName</td>
       <td>@item.DateOfBirth</td>
       <td>@item.Address</td>
     </tr>
   }
   </tbody>
</table>
